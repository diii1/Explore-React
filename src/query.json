{
    query siswa {
        anggota {
                nama
                keterangans(where: {status: {_eq: true}}) {
                nilai
                pelajaran
                status
            }
        }
    }
    
    mutation insert {
        insert_keterangan(objects: {id: 5, id_anggota: 3, nilai: 90, pelajaran: "Sistem Informasi", status: true}) {
            returning {
                id
                id_anggota
                nilai
                pelajaran
                status
            }
        }
    }
    
    mutation insert2 {
        insert_keterangan_one(on_conflict: {constraint: keterangan_pkey, where: {}}, object: {id: 6, id_anggota: 3, nilai: 55, pelajaran: "React", status: false}) {
            id
            id_anggota
            nilai
            pelajaran
            status
        }
    }
    
    mutation update {
        update_keterangan(where: {status: {_eq: false}}, _set: {status: true}) {
            affected_rows
            returning {
                id
                id_anggota
                nilai
                pelajaran
                status
            }
        }
    }
}